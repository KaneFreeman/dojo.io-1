{
	"build-app": {
		"pwa": {
			"manifest": {
				"name": "dojo.io",
				"short_name": "dojo.io",
				"description": "dojo.io",
				"background_color": "#ffffff",
				"theme_color": "#222127",
				"icons": [
					{
						"src": "src/favicon.ico",
						"sizes": [
							128,
							256,
							512
						]
					}
				]
			},
			"serviceWorker": {
				"clientsClaim": true,
				"routes": [
					{
						"urlPattern": "https://fonts.googleapis.com/css",
						"strategy": "networkFirst"
					},
					{
						"urlPattern": "https://fonts.gstatic.com/s/montserrat/v12",
						"strategy": "networkFirst"
					}
				]
			}
		}
	},
	"test-intern": {},
	"create-app": {}
}
